{
    "cache_bucket_name": "reporting_cache",
    "output_bucket_name": "reporting_loughborough",
    "logfile": "logs/oagen.log",
    "loglevel": "DEBUG",
    "analytics": [
        [
            "get_data",
            {
                "current_table": "academic-observatory.institution.institutions_latest_oacites"
            }
        ],
        [
            "plot_graphs",
            {
                "depends" : ["get_data"],
                "focus_year": 2018,
                "year_range": [
                    2005,
                    2019
                ],
                "identifier": "grid.6571.5",
                "comparison": [
                    "grid.83440.3b",
                    "grid.5335.0",
                    "grid.8756.c",
                    "grid.7107.1",
                    "grid.11914.3c",
                    "grid.11201.33"
                ]
            }
        ]
    ],
    "template_file": "report_cn.md",
    "output_basename": "{{ reporting['identifier'] }}_oa_report",
    "filters": [
        [
            "markdown",
            "html"
        ],
        [
            "xhtml2pdf",
            "pdf"
        ]
    ]
}